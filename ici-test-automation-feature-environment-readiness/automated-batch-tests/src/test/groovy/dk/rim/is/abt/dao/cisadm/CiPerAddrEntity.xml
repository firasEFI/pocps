        <attributes>
            <id name="perId">
                <column name="PER_ID" length="10"/>
            </id>
            <id name="seqNum">
                <column name="SEQ_NUM" precision="3"/>
            </id>
            <basic name="addressId">
                <column name="ADDRESS_ID" length="14"/>
            </basic>
            <basic name="addressTypeXflg">
                <column name="ADDRESS_TYPE_XFLG" length="30"/>
            </basic>
            <basic name="startDt">
                <column name="START_DT" precision="-1"/>
            </basic>
            <basic name="endDt">
                <column name="END_DT" nullable="false" precision="-1"/>
            </basic>
            <basic name="seasonStartMmdd">
                <column name="SEASON_START_MMDD" nullable="false" length="4"/>
            </basic>
            <basic name="seasonEndMmdd">
                <column name="SEASON_END_MMDD" nullable="false" length="4"/>
            </basic>
            <basic name="addressPrioFlg">
                <column name="ADDRESS_PRIO_FLG" nullable="false" length="4"/>
            </basic>
            <basic name="deliverableFlg">
                <column name="DELIVERABLE_FLG" length="4"/>
            </basic>
            <basic name="version">
                <column name="VERSION" precision="5"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
