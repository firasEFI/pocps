<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="dk.rim.is.abt.dao.cisadm.IntegrationFileTypeEntity">
        <table name="INTEGRATION_FILE_TYPE" schema="CISADM" catalog=""/>
        <attributes>
            <id name="id">
                <column name="ID" precision="20"/>
            </id>
            <basic name="version">
                <column name="VERSION" precision="20"/>
            </basic>
            <basic name="externalSystemName">
                <column name="EXTERNAL_SYSTEM_NAME" length="50"/>
            </basic>
            <basic name="fileTypeName">
                <column name="FILE_TYPE_NAME" length="50"/>
            </basic>
            <basic name="sourceSystemName">
                <column name="SOURCE_SYSTEM_NAME" length="50"/>
            </basic>
            <basic name="destinationSystemName">
                <column name="DESTINATION_SYSTEM_NAME" length="50"/>
            </basic>
            <basic name="fileFormat">
                <column name="FILE_FORMAT" length="50"/>
            </basic>
            <basic name="direction">
                <column name="DIRECTION" length="50"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
